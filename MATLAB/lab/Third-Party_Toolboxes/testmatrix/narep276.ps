	4 -2 roll moveto
	dup  0 exch rlineto
	exch 0 rlineto
	neg  0 exch rlineto
	closepath
	} bdef
/FR {	% FrameRect
	MR stroke
	} bdef
/PR {	% PaintRect
	MR fill
	} bdef
/L1i {	% Level 1 Image
	{ currentfile picstr readhexstring pop } image
	} bdef

/half_width 0 def
/half_height 0 def
/MakeOval {
	newpath
	/ury xstore /urx xstore /lly xstore /llx xstore
	/half_width  urx llx sub 2 div store
	/half_height ury lly sub 2 div store
