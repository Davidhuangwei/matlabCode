analDirs = {...
    '/BEEF02/smm/sm9614_Analysis/analysis02/',...
    '/BEEF02/smm/sm9608_Analysis/analysis02',...
    '/BEEF01/smm/sm9601_Analysis/analysis03/'...
    '/BEEF01/smm/sm9603_Analysis/analysis04/'...
    }
fileExtCell = {...
    '.eeg',...
    '_LinNearCSD121.csd',...
    '_NearAveCSD1.csd',...
    }
thetaFreqRange = [6 12];

for j=1:length(analDirs)
    cd(analDirs{j})
    CalcCohPeak01(LoadVar('MazeFiles'),fileExtCell,thetaFreqRange);
end
return

analDirs = {...
    %'/BEEF02/smm/sm9614_Analysis/analysis02/',...
    %'/BEEF02/smm/sm9608_Analysis/analysis02',...
    %'/BEEF01/smm/sm9601_Analysis/analysis03/'...
    '/BEEF01/smm/sm9603_Analysis/analysis04/'...
    }
fileExtCell = {...
    '.eeg',...
    '_LinNearCSD121.csd',...
    '_NearAveCSD1.csd',...
    }
for j=1:length(analDirs)
    cd(analDirs{j})
    fileBaseMat = LoadVar('MazeFiles');
    for k=1:size(fileBaseMat,1)
        %cd(fileBaseMat(k,:))
        for m=1:length(fileExtCell)
            fprintf(['!mv ' fileBaseMat(k,:) '/CalcRunningSpectra9__noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} ' ' ...
                fileBaseMat(k,:) '/CalcRunningSpectra9_noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} '\n'])
            fprintf(['!mv ' fileBaseMat(k,:) '/CalcRunningSpectra9__noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} ' ' ...
                fileBaseMat(k,:) '/CalcRunningSpectra9_noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} '\n'])
%             eval(['!mv ' fileBaseMat(k,:) '/CalcRunningSpectra9__noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} ' ' ...
%                 fileBaseMat(k,:) '/CalcRunningSpectra9_noExp_MidPoints_MinSpeed0Win626' fileExtCell{m} ])
        end
        %cd ..
    end
end

