fprintf('Saving: %s\n',[pwd '/' outName])

save(outName,SaveAsV6,'trialDesig','categIndepNames','contIndepNames',...
    'contVarSub','fileBaseCell','trialMeanBool','outlierDepth',...
    'modelSpec','ssType','equalNbool');
return

