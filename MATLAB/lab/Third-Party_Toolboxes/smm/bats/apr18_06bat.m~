                }
analRoutine = {...
                'Alter_S0_A0',...
                'Alter_Vs_Control_EachRegion_S0_A0',...
                'Alter_Right_Vs_Left_S0_A0',...
                
                };
for m=1:length(analRoutine)
    for j=1:length(fileExtCell)
        for k=1:length(analDirs)
            cd(analDirs{k})
            GlmPartialAnalysisBatch04('CalcRunningSpectra9_noExp',analRoutine{m},'01',{'MazeFiles'},fileExtCell{j})
        end
    end
end

for m=1:length(analRoutine)
    for j=1:length(fileExtCell)
        for k=1:length(analDirs)
            cd(analDirs{k})
            GlmWholeAnalysisBatch04('CalcRunningSpectra9_noExp',analRoutine{m},'01',{'MazeFiles'},fileExtCell{j})
        end
    end
