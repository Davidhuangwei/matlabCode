for j=1:length(analDirs)
    files = union(LoadVar([analDirs{j} 'FileInfo/MazeFiles']),LoadVar([analDirs{j} 'FileInfo/RemFiles']));
    %files = LoadVar([analDirs{j} 'FileInfo/MazeFiles']);
 %   files = LoadVar([analDirs{j} 'FileInfo/RemFiles']);
    for k=1:length(files)
        %dir([analDirs{j} files{k} '/' spectralDirBase '*'])
        rmText = ['!rm -r ' analDirs{j} files{k} '/' spectralDirBase '*'];
        fprintf([rmText '\n']);
        eval(rmText);
    end
end

